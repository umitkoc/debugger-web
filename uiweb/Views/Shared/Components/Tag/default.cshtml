@model ICollection<Tag>


@if (Model.Count() == 0)
{
    <div class="alert alert-bg-danger">Not Tag</div>
}
else
{
    foreach (var item in Model)
    {

        <a asp-controller="home" asp-action="tagsearch" asp-route-id=@item.id
     class="btn btn-@item.color btn-sm py-0">@item.name</a>
    }

}
