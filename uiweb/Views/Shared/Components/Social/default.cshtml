@model ICollection<Social>


@if (Model.Count() == 0)
{
    <div class="alert alert-bg-danger">Not Social</div>
}
else
{
    foreach (var item in Model)
    {
        <a href="@item.url" target="_blank">
            <i class="@item.icon"></i>
        </a>

    }

}



<a data-bs-toggle="modal" data-bs-target="#exampleModal"> <i class="fas fa-file-pdf fa-2x"></i></a>